<!-- -*- xml -*-
     THis launches the ros/visp/auto/tracker node
  -->
<launch>
  

<!-- starte game master node-->
<node pkg="game_master" type="game_master" name="game_master" output="screen" />


<!-- starte ai node-->
<node pkg="ai" type="ai" name="ai" output="screen" />

<!-- starte grasping_baxter launch-->
<include file="$(find grasping_baxter)/launch/grasping.launch" launch-prefix="xterm -e gdb --args"/>

<!-- starte camera launch-->
<include file="$(find camera)/launch/camera.launch"/>

</launch>
